{
  "name": "boilerplate-react",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node scripts/customized-config start",
    "build": "node scripts/customized-config build",
    "test": "yarn lint && yarn jest",
    "lint": "yarn eslint && yarn stylelint",
    "eslint": "NODE_ENV=test eslint src stories tests *.js",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "stylelint": "NODE_ENV=test stylelint 'src/**/*.css'",
    "eject": "react-scripts eject"
  },
  "jest": {
    "verbose": true,
    "moduleNameMapper": {
      "^.+\\.(css)$": "identity-obj-proxy",
      "^.+\\.(png|svg|txt|ico)$": "<rootDir>/tests/empty-module.js",
      "^_modules/(.*)": "<rootDir>/src/modules/$1",
      "^_components/(.*)": "<rootDir>/src/components/$1",
      "^_config/(.*)": "<rootDir>/src/config/$1",
      "^_services/(.*)": "<rootDir>/src/services/$1",
      "^_views/(.*)": "<rootDir>/src/views/$1",
      "^_utils/(.*)": "<rootDir>/src/utils/$1",
      "^_styles/(.*)": "<rootDir>/src/styles/$1",
      "^_hocs/(.*)": "<rootDir>/src/hocs/$1",
      "^_middlewares/(.*)": "<rootDir>/src/middlewares/$1",
      "^_assets/(.*)": "<rootDir>/src/assets/$1",
      "\\.worker.js": "<rootDir>/tests/__mocks__/workerMock.js",
      "^_privateDependencies/(.*)": "<rootDir>/private-dependencies/$1"
    },
    "moduleFileExtensions": [
      "js"
    ],
    "setupFiles": [
      "./tests/config.test.js"
    ],
    "testPathIgnorePatterns": [
      "./tests/config.test.js",
      "./private-dependencies",
      "<rootDir>[/\\\\](dist|dist-server|node_modules|.storybook)[/\\\\]"
    ],
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ]
  },
  "dependencies": {
    "@storybook/addon-actions": "^3.4.3",
    "classnames": "^2.2.5",
    "history": "^4.7.2",
    "normalize.css": "^8.0.0",
    "prop-types": "^15.6.0",
    "proxyquire": "^1.8.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-scripts": "^1.1.0",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "^5.0.0",
    "redux-thunk": "^2.2.0",
    "rewire": "^3.0.2",
    "suitcss-components-grid": "^4.0.0",
    "suitcss-utils-after": "^1.0.1",
    "suitcss-utils-before": "^1.0.1",
    "suitcss-utils-size": "^2.0.1"
  },
  "devDependencies": {
    "@storybook/react": "^3.3.14",
    "autoprefixer": "^7.2.5",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^20.0.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.12",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.9",
    "enzyme": "^3.3.0",
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-import-resolver-webpack": "^0.8.4",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.6.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^20.0.0",
    "postcss-css-variables": "^0.8.0",
    "postcss-custom-media": "^6.0.0",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.1.0",
    "postcss-nested": "^3.0.0",
    "prettier": "^1.10.2",
    "raf-polyfill": "^1.0.0",
    "redux-devtools-extension": "^2.13.2",
    "style-loader": "^0.20.1",
    "stylelint": "^8.4.0",
    "stylelint-config-standard": "^18.0.0",
    "svg-sprite-loader": "^3.6.2",
    "webpack": "^3.10.0"
  }
}
